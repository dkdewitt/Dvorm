	"dependencies": {
		"dvorm:orm": "*",
		"dvorm:vibeproviders": "*",
		"dvorm:email": "*",
		"dvorm:mysql": "*"
	},
	"sourcePaths": ["src/"],
	"subPackages": [
		{
			"name": "orm",
			"targetType": "library",
			"sourcePaths": ["src/base/dvorm"],
			"importPaths": ["src/base"]
		},
		{	
			"name": "vibeproviders",
			"targetType": "library",
			"sourcePaths": ["src/vibeproviders/dvorm"],
			"importPaths": ["src/vibeproviders"],
			"dependencies": {
				"dvorm:orm": "*",
				"vibe-d": "~>0.7.22"
			}
		},
		{
			"name": "test",
			"dependencies": {
				"dvorm:orm": "*",
				"dvorm:vibeproviders": "*"
			},
			"sourcePaths": [],
			"sourceFiles": ["src/main.d"],
			"versions": ["VibeCustomMain"]
		},
		{	
			"name": "email",
			"targetType": "library",
			"sourcePaths": ["src/email/dvorm"],
			"importPaths": ["src/email"],
			"dependencies": {
				"dvorm:orm": "*",
				"vibe-d": "~>0.7.22"
			}
		},
		{
			"name": "mysql",
			"targetType": "library",
			"sourcePaths": ["src/mysql/dvorm"],
			"importPaths": ["src/mysql"],
			"dependencies": {
				"dvorm:orm": "*",
				"vibe-d": "~>0.7.22",
				"mysql-native": "~>0.0.16"
			}
		}
	],
	"configurations": [
		{
			"name": "default"
		},
		{
			"name": "forvibe",
			"versions": ["VibeCustomMain"]
		}
	]
}
